<idea-plugin>
    <id>export</id>
    <name>Export</name>
    <version>1.0</version>
    <vendor url="https://github.com/jashion666/ExportProject/tree/master">项目地址</vendor>

    <description><![CDATA[
      export project files.<br>
    ]]></description>

    <change-notes><![CDATA[
      . 20190611 初次提交
    ]]>
    </change-notes>

    <idea-version since-build="173.0"/>

    <extensions defaultExtensionNs="com.intellij">
    </extensions>

    <actions>
        <!-- Add your actions here -->
        <action id="CreatePatcherAction.createPatcher" class="com.jashion.main.CreatePatcherAction"
                text="Export">
            <add-to-group group-id="ProjectViewPopupMenu" anchor="after" relative-to-action="ReplaceInPath"/>
        </action>
    </actions>

</idea-plugin>